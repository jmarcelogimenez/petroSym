FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [ 0 2 -3 0 0 0 0 ];

internalField   uniform 1;

boundaryField
{
	fixedWalls
    {
        type            zeroGradient;
    }
    movingWall
    {
        type            zeroGradient;
    }
    frontAndBack
    {
        type            empty;
    }
}


